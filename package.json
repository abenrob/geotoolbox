{
  "name": "geotoolbox",
  "version": "1.9.0",
  "description": "geotoolbox is javascript tool for geographers based on d3geo, topojson and jsts. It allows to simply deal with geojson properties and provides several GIS operations useful for thematic cartography.",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "exports": {
    "umd": "./dist/index.min.js",
    "default": "./src/index.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocarto/geotoolbox.git"
  },
  "keywords": [
    "GIS",
    "geospatial",
    "geocomputing",
    "geography"
  ],
  "author": "Nicolas Lambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neocarto/geotoolbox/issues"
  },
  "homepage": "https://github.com/neocarto/geotoolbox#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "d3-geo": "^3.0.1",
    "jsts": "^2.7.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3"
  }
}
